{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "require-jsdoc": 0,
    "no-console": 0,
    "valid-jsdoc": 0,
    "no-undef": 0,
    "no-unused-vars": 0
  },
  "globals": {
    "phaser": true
  }
}
